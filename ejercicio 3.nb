   3.6542580109572*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"C", "[", "1", "]"}], "\[Element]", "Integers"}], "&&", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"t", "\[Equal]", 
     RowBox[{
      RowBox[{"-", 
